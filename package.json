{
  "name": "themenal",
  "version": "1.0.0",
  "description": "A beautiful terminal theme manager for Debian/Ubuntu Linux",
  "main": "dist/cli/index.js",
  "type": "module",
  "bin": {
    "themenal": "./bin/themenal"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc && cp -r src/themes dist/themes",
    "start": "node dist/cli/index.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabrielsoac/therminal.git"
  },
  "keywords": [
    "linux",
    "terminal",
    "theme",
    "colors",
    "debian",
    "ubuntu",
    "gnome-terminal",
    "cli"
  ],
  "author": "Gabriel Soares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gabrielsoac/therminal/issues"
  },
  "homepage": "https://github.com/Gabrielsoac/therminal#readme",
  "dependencies": {
    "js-yaml": "^4.1.1",
    "terminal-kit": "^3.1.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/terminal-kit": "^2.5.7",
    "@types/yargs": "^17.0.35",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}
